    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "shx": "^0.3.2",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
